{
  "tasks": [
    {
      "args": [
        "run",
        "--release",
        "--",
        "--dev"
      ],
      "command": "cargo",
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "label": "Run ",
      "presentation": {
        "panel": "new",
        "reveal": "always"
      },
      "problemMatcher": [
        {
          "fileLocation": [
            "relative",
            "${workspaceRoot}"
          ],
          "owner": "rust",
          "pattern": {
            "column": 3,
            "endColumn": 5,
            "endLine": 4,
            "file": 1,
            "line": 2,
            "message": 7,
            "regexp": "^(.*):(\\d+):(\\d+):\\s+(\\d+):(\\d+)\\s+(warning|error):\\s+(.*)$",
            "severity": 6
          }
        }
      ],
      "type": "shell"
    }
  ],
  "version": "2.0.0"
}
